    "build_dep": "npm run-script backbone-orm build",
    "watch_dep": "npm run-script backbone-orm watch"
  },
  "devDependencies": {
    "async": "^0.9.0",
    "body-parser": "1.x",
    "coffee-script": "*",
    "event-stream": "^3.1.7",
    "express": "4.x",
    "gulp": "^3.8.6",
    "gulp-coffee": "^2.1.1",
    "gulp-install": "^0.1.8",
    "gulp-spawn-mocha": "^0.1.7-beta.3",
    "gulp-util": "^3.0.0",
    "mocha": "^1.20.1",
    "powerset": "0.0.x",
    "restify": "2.x",
    "supertest": "*"
  },
  "dependencies": {
    "underscore": "1.6.x",
